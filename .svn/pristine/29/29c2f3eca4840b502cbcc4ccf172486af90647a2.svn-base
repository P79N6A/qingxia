.tabbtn {
    height:39px;
    background-color: #62BD65;
    border-left:solid 1px #ddd;
    border-right:solid 1px #ddd;
}
.tabbtn li {
    float:left;
    position:relative;
    margin:0 0 0 -1px;
}
.tabbtn li a {
    display:block;
    float:left;
    height:39px;
    line-height:39px;
    overflow:hidden;
    width:128px;
    text-align:center;
    cursor:pointer;
    color: #FFF;
    font-weight: 600;
}
.tabbtn li a:hover {text-decoration: none;}
.tabbtn li.current {
    border-left:solid 1px #d5d5d5;
    border-right:solid 1px #d5d5d5;
    border-top:solid 1px #c5c5c5;
}
.tabbtn li.current a {
    border-top:solid 3px #ff6600;
    height:35px;
    line-height:35px;
    background:#fff;
    color:#17A31C;
    font-weight:800;
}
