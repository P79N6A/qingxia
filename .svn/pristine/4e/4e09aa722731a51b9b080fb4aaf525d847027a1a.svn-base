/*弹窗*/
.weui_mask1 {
	transition-duration: .3s;
	position: fixed;
	z-index: 10;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.6);
	z-index: 100;
}
.weui_dialog1{
	-webkit-transition-duration: .2s;
	transition-duration: .2s;
	-webkit-transform: scale(0.9);
	transform: scale(0.9);
	margin: 0;
	left: 7.5%;
	top: 30%;
	z-index: 100;
	position: fixed;
	width: 85%;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	background-color: #FAFAFC;
	text-align: center;
	border-radius: 3px;
}
